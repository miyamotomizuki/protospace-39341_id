<div class="main">
  <div class="inner">
    <% if user_signed_in? && current_user.id == @tweet.user_id %>
      <div class="form__wrapper">
        <h2 class="page-heading">プロトタイプ編集</h2>
        <%= render partial: "form", locals: { prototype: @prototype } %>
      </div>
    <% else %>
      <% redirect_to root_path
    <% end %>
  </div>
</div>